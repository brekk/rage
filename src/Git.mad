import Wish from "Wish"

import { git } from "@/External"



export clone = pipe(
  of,
  mappend(["clone"]),
  git,
)

export init = git(["init"])

export setRemote = (repo) => git(["remote", "add", "origin", repo])

export pushUpstream = (origin, branch) => git(["push", "--set-upstream", origin, branch])

export gpsup = pushUpstream("origin", "main")
