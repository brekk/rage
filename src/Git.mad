import Wish from "Wish"

import { git } from "@/External"



export clone = pipe(
  of,
  mappend(["clone"]),
  git,
)

export init = (folder) => git(["-C", folder, "init"])

export setRemote = (folder, repo) => git(["-C", folder, "remote", "add", "origin", repo])

