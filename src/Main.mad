import { always, noop } from "Function"
import IO from "IO"
import { fulfill } from "Wish"

import Ext from "@/External"



main = () => {
  undertaker = fulfill(
    pipe(
      IO.pTrace("BAD!"),
      noop,
    ),
    pipe(
      IO.put,
      noop,
    ),
  )

  pipe(
    Ext.walk($, ["status"]),
    map(.stdout),
    undertaker,
  )("git")
}
